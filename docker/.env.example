# MySQL Database Dumper - Environment Variables
# Copy this file to .env and update with your values

# Primary MySQL Instance
MYSQL_PRIMARY_HOST=localhost
MYSQL_PRIMARY_PORT=3306
MYSQL_PRIMARY_USER=root
MYSQL_PRIMARY_PASSWORD=your_secure_password

# Secondary MySQL Instance (optional)
MYSQL_SECONDARY_HOST=
MYSQL_SECONDARY_PORT=3306
MYSQL_SECONDARY_USER=
MYSQL_SECONDARY_PASSWORD=

# Timezone
TZ=UTC
